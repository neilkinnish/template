{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": [],
  "plugins": [],
  "rules": {
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "react/require-default-props": [0],
    "import/first": [0],
    "no-unused-expressions": [0],
    "arrow-parens": [0]
  }
}
